
   {
    "name": "RichGitHub",
    "version": "1.1",
    "description": "A browser action with a popup dump of all bookmarks, including search, add, edit and delete.",
    "permissions": [
      "bookmarks",
      "background",
      "browsingData",
      "tabs",
      "webRequest",
      "activeTab"
    ],
    "browser_action": {
        "default_title": "My Bookmarks",
        "default_icon": "firewolf.png",
        "default_popup": "newTab.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["content.css"],
          "js": ["extension.js"]
        }
      ],
    "manifest_version": 2
  }